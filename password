#include<stdio.h>
int main()
{
	char name[20];
	char password[10];
	printf("enter username: ");
	scanf("%s",name);
	printf("enter password: ");
	scanf("%s",password);
	if(strcmp(name,"pooja")==0 && strcmp(password,"0911")==0) 
	printf("Access granted\n");
	else printf("Access denied\n");
	return 0;
}
